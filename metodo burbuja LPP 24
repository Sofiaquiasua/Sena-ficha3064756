arreglo[10]de cadena [15]kiko
arreglo[10]de entero numero
entero descanso, fila, columna, temporal, pichon 
inicio
	escriba "ingrese los elementos"
	llamar nueva_linea

		para descanso<-1 hasta 10 haga 
		llamar nueva_linea 
		escriba "ingrese el numero para la posicion [",descanso,"]"
		lea numero[descanso]
		fin para 
		llamar nueva_linea
	para descanso<-1 hasta 10 haga 
	llamar nueva_linea 
	escriba  "en la posicion [",descanso,"] usted ingreso el elemento", numero [descanso]
	fin para 
   llamar nueva_linea

para fila<-1 hasta 10 haga
	para columna<-1 hasta 10-fila haga
		si(numero[columna]>numero[columna+1])entonces
			temporal<-numero[columna]
			numero[columna]<-numero[columna+1]
			numero[columna+1]<-temporal
				
		fin si
	fin para
fin para

   llamar nueva_linea
 escriba "El sistema organizado es:"
    llamar nueva_linea
 para descanso<-1 hasta 10 haga
  	llamar nueva_linea
  	escriba "la posicion [",descanso,"] es ", numero [descanso]

	llamar nueva_linea
 fin para






